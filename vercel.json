{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "pages/api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(favicon.ico|robots.txt)",
        "dest": "/static/$1"
      },
      {
        "src": "/_next/(.*)",
        "dest": "/_next/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/pages/$1"
      }
    ]
  }
  